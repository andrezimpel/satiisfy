<h1>Create your new Satiisfy Account, Bruh</h1>

<%= simple_form_for(@account, url: "/accounts", method: "POST") do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>

    <%= f.fields_for :users do |u| %>
      <%= u.input :email %>
      <%= u.input :password %>
      <%= u.input :password_confirmation %>
    <% end %>
    <br/>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "btn-primary" %>
  </div>
<% end %>
